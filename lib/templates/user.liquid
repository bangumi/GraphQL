<html lang='en'>
<head>
  {% include 'partial/head' %}
</head>
<body>
{% include 'partial/header' %}

<div class='container mt-5'>
  <div class='container mt-5'>
    {{ user.username }} 已经登录
    <button class='btn btn-danger' onclick='logout()'>登出</button>
  </div>
  <script>
    async function logout() {
      await fetch('/p1/logout', { method: 'post' });
      location.reload();
    }
  </script>
</div>
</body>
</html>
